# Database configuration
DB_HOST=172.25.0.101  # Fixed IP for PostgreSQL container in the public network
DB_PORT=5432
DB_NAME=DBopenAPI
DB_SCHEMA=openapi_schema
DB_USER=openapi_user
DB_PASSWORD=your_db_password_here
DB_ROOT_USER=postgres
DB_ROOT_PASSWORD=your_root_password_here

# Connection string built from the above parameters
DATABASE_URL=postgresql://${DB_USER}:${DB_PASSWORD}@${DB_HOST}:${DB_PORT}/${DB_NAME}?options=-csearch_path%3D${DB_SCHEMA}

# OpenAPI configuration
# IMPORTANT: Update this URL with the actual OpenAPI base URL
OPENAPI_BASE_URL_RISK=https://api.risk.openapi.com/v1
# IMPORTANT: Set the actual token provided by the OpenAPI service
OPENAPI_TOKEN_RISK=your_api_token_here
TIMEZONE=Europe/Rome

# Debugging settings - Set to True in development to see more logs
DEBUG=False
